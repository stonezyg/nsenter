#/bin/sh
#TEXT: shell set timeout 
#Return  0:True, 1:False, 2 N/A


# ==== Check N/A conditions ====

if ! [ -f /etc/profile ] > /dev/null 
then 
  echo "== N/A ==" 
  exit 2  
fi

# ==== Check FAILED conditions ====
if ! grep -q 'export TMOUT=600' /etc/profile >> /dev/null 
then
  echo "== Failed =="
  exit 1
fi

# ==== Success Remains ====
echo "== Success =="
exit 0

